{
	"name": "vscode-m5stack-mpyreader",
	"displayName": "vscode-m5stack-mpyreader",
	"description": "A extension to manage files for M5Stack Micropython system.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "m5stack",
					"name": "M5Stack Device"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"serialport": "^7.1.4"
	}
}
